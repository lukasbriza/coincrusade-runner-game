{
  "name": "@lukasbriza/backend",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "prebuild": "rimraf build tsconfig.build.tsbuildinfo",
    "build": "nest build",
    "cleanup": "rimraf .eslintcache .turbo tsconfig.tsbuildinfo tsconfig.build.tsbuildinfo build node_modules  --glob",
    "predev": "rimraf build tsconfig.tsbuildinfo tsconfig.build.tsbuildinfo & docker-compose up -d",
    "dev": "nest start --watch & docker-compose up -d",
    "start": "NODE_ENV=production node build/main",
    "prisma:generate": "prisma generate",
    "fix": "run-p ts format:fix lint:fix",
    "format": "prettier --check --ignore-path .prettierignore \"./**/*.{graphql,json,md}\"",
    "format:fix": "pnpm run format --write",
    "lint": "eslint --cache --ext cjs,js,ts .",
    "lint:fix": "pnpm run lint --fix",
    "ts": "tsc --build tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@lukasbriza/eslint-config": "workspace:*",
    "@lukasbriza/ts-config": "workspace:*",
    "@nestjs/common": "10.3.10",
    "@nestjs/config": "3.2.3",
    "@nestjs/core": "10.3.10",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.3.10",
    "@nestjs/swagger": "7.4.0",
    "@prisma/client": "5.21.1",
    "@tensorflow/tfjs": "4.20.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "passport": "0.7.0",
    "passport-headerapikey": "1.2.2",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.2",
    "@nestjs/schematics": "10.1.3",
    "@nestjs/testing": "10.3.10",
    "@types/cookie-parser": "1.4.7",
    "@types/express": "4.17.21",
    "@types/multer": "1.4.11",
    "@types/node": "20.14.9",
    "@types/passport": "1.0.16",
    "@types/passport-http": "0.3.11",
    "@types/supertest": "6.0.2",
    "mongodb": "6.8.0",
    "prisma": "5.17.0",
    "source-map-support": "^0.5.21",
    "supertest": "7.0.0",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.3",
    "vitest": "2.0.4"
  }
}
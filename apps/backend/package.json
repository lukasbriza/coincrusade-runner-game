{
  "name": "@lukasbriza/backend",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "prebuild": "rimraf build tsconfig.build.tsbuildinfo",
    "build": "nest build",
    "cleanup": "rimraf .eslintcache .turbo tsconfig.tsbuildinfo tsconfig.build.tsbuildinfo build node_modules  --glob",
    "predev": "rimraf build tsconfig.tsbuildinfo tsconfig.build.tsbuildinfo",
    "dev": "dotenv -e .env.dev -- nest start --watch",
    "dev-db-start": "docker-compose --env-file .env.dev up -d -V",
    "fix": "run-p ts format:fix lint:fix",
    "format": "prettier --check --ignore-path .prettierignore \"./**/*.{graphql,json,md}\"",
    "format:fix": "pnpm run format --write",
    "lint": "eslint --cache --ext cjs,js,ts .",
    "lint:fix": "pnpm run lint --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate-dev": "dotenv -e .env.dev -- pnpm prisma migrate dev",
    "prisma:migrate-local": "dotenv -e .env.local -- pnpm prisma migrate dev",
    "prisma:push-dev": "dotenv -e .env.dev -- pnpm prisma db push",
    "prisma:push-local": "dotenv -e .env.local -- pnpm prisma db push",
    "start": "node build/main",
    "ts": "tsc --build tsconfig.json"
  },
  "dependencies": {
    "@lukasbriza/eslint-config": "workspace:*",
    "@lukasbriza/ts-config": "workspace:*",
    "@nestjs-modules/mailer": "2.0.2",
    "@nestjs/common": "11.0.1",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.0.1",
    "@nestjs/platform-express": "11.0.0",
    "@nestjs/platform-socket.io": "11.0.20",
    "@nestjs/swagger": "11.1.4",
    "@nestjs/websockets": "11.0.20",
    "@prisma/client": "5.21.1",
    "@tensorflow/tfjs": "4.20.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "nestjs-asyncapi": "1.4.0",
    "nodemailer": "6.10.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "socket.io": "4.8.1",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.0",
    "@nestjs/cli": "11.0.0",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.0",
    "@types/cookie-parser": "1.4.7",
    "@types/express": "4.17.21",
    "@types/multer": "1.4.11",
    "@types/node": "20.14.9",
    "@types/nodemailer": "6.4.17",
    "@types/supertest": "6.0.2",
    "dotenv-cli": "7.4.2",
    "mongodb": "6.8.0",
    "prisma": "5.21.1",
    "source-map-support": "^0.5.21",
    "supertest": "7.0.0",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.3",
    "vitest": "2.0.4"
  }
}
